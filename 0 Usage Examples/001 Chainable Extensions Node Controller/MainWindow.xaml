<Window x:Class="Aki32Utilities.UsageExamples.ChainableExtensionNodeController.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Aki32Utilities.UsageExamples.ChainableExtensionNodeController"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1700">
    <Grid>
        <Grid x:Name="container">

        </Grid>
        <StackPanel VerticalAlignment="Bottom" Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Panel.ZIndex="1000" Width="100" Height="25" Content="Run" Click="runClick"/>
            <Button Panel.ZIndex="1000" Width="100" Height="25" Content="Add" Click="addClick"/>
            <Button Panel.ZIndex="1000" Width="100" Height="25" Content="ReArrange" Click="rearrange"/>
            <Button Panel.ZIndex="1000" Width="100" Height="25" Content="Rotate" Click="rotate"/>
        </StackPanel> 
    </Grid>
</Window>
